(function() {
  this.html("<%= escape_javascript render(partial: 'vote', locals: {vote: @vote}) %>");
  var submit = this.find('input:submit');
  var form = this.find('form');
  submit.hide();
  this.dialog('option', 'buttons', [{
    text: submit.val(),
    click: function() {
      form.submit();
    }
  }]).dialog('open');
}).call($('#comment-dialog'));